{
    "compilerOptions": {
        "esModuleInterop": true,
        "jsx": "react-jsx",
        "module": "ES2022",
        "target": "ES2022",
        "moduleResolution": "node",
        "strict": true,
        "noFallthroughCasesInSwitch": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "skipLibCheck": true,
        "noEmit": true,
        "baseUrl": "./",
        "paths": {
            "@/*": [
                "src/*"
            ]
        },
        "types": [
            "vite/client"
        ],
    },
    "include": [
        "src/**/*",
        "vite.config.ts"
    ]
}